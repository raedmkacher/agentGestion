{% extends 'base.html.twig' %}

{% block title %}UserAddress{% endblock %}

{% block body %}
    <h1>UserAddress</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ user_address.id }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ user_address.country }}</td>
            </tr>
            <tr>
                <th>State</th>
                <td>{{ user_address.state }}</td>
            </tr>
            <tr>
                <th>AddressLine</th>
                <td>{{ user_address.addressLine }}</td>
            </tr>
            <tr>
                <th>ZipCode</th>
                <td>{{ user_address.zipCode }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_user_address_index') }}">back to list</a>

    <a href="{{ path('app_user_address_edit', {'id': user_address.id}) }}">edit</a>

    {{ include('user_address/_delete_form.html.twig') }}
{% endblock %}
